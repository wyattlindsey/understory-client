{
  "name": "understory-client",
  "version": "1.0.0",
  "description": "frontend for understory application",
  "engines": {
    "node": ">=6.9.1"
  },
  "main": "server/index.js",
  "scripts": {
    "build": "yarn build:client; yarn build:server",
    "build:prod": "export NODE_ENV=production; yarn build",
    "build:client": "webpack --config config/webpack/client.js",
    "build:server": "webpack --config config/webpack/server.js",
    "serve:dev": "export NODE_ENV=development; nodemon ./server/index.js",
    "serve:prod": "export NODE_ENV=production; node ./server/index.js",
    "start": "yarn serve:prod",
    "start:dev":
      "export NODE_ENV=development; webpack-dev-server --config config/webpack/client.js",
    "start:prod": "export NODE_ENV=production; yarn build; yarn serve:prod",
    "heroku-postbuild": "yarn build:prod",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyattlindsey/understory-client.git"
  },
  "author": "Wyatt Lindsey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wyattlindsey/understory-client/issues"
  },
  "homepage": "https://github.com/wyattlindsey/understory-client#readme",
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "hbs": "^4.0.1",
    "html-webpack-exclude-assets-plugin": "^0.0.5",
    "html-webpack-plugin": "^2.30.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.12",
    "object-assign": "^4.1.1",
    "postcss": "^6.0.16",
    "postcss-advanced-variables": "^2.3.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.0.10",
    "postcss-nested": "^3.0.0",
    "postcss-partial-import": "^4.1.0",
    "promise": "^8.0.1",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "serve-favicon": "^2.4.5",
    "style-loader": "^0.20.1",
    "three": "^0.89.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "whatwg-fetch": "^2.0.3"
  }
}
